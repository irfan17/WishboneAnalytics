      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2020-10-21</actionDate>
          <actionDesc>Introduced in Senate</actionDesc>
          <lastSummaryUpdateDate>2020-12-21T15:23:23Z</lastSummaryUpdateDate>
          <text><![CDATA[<p><b>Ensuring Access to COVID-19 Vaccines for Seniors Act</b></p> <p>This bill specifies that vaccines for COVID-19 (i.e., coronavirus disease 2019) that are authorized for emergency use must be covered under Medicare without cost-sharing.</p> <p>The bill applies retroactively.</p>]]></text>
          <updateDate>2020-10-21T04:00:00Z</updateDate>
          <name>Introduced in Senate</name>
        </item>
      </billSummaries>
